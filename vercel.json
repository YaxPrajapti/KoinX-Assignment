{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/fetch/(.*)",
        "dest": "router/transactionRouter.js"
      },
      {
        "src": "/api/expense/(.*)",
        "dest": "router/expenseRouter.js"
      }
    ]
  }
  